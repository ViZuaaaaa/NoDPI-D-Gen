{
  "proxy": {
    "host": "127.0.0.1",
    "port": 8881
  },
  "pac": {
    "port": 8882
  },
  "log": {
    "path": "dgen-nodpi4.log"
  },
  "upstream": {
    "enabled": false,
    "host": "",
    "port": 0
  },
  "verbose": false,
  "console": {
    "mode": "stats"
  },
  "domains": {
    "matching": "strict"
  },
  "fragment": {
    "enabled": true,
    "min_parts": 2,
    "max_parts": 6,
    "strategy": "random_parts",
    "fixed_parts": 4,
    "chunk_size": 1200,
    "tiny_first_min": 1,
    "tiny_first_max": 5,
    "sni_cut_pad": 2,
    "jitter_ms_min": 0,
    "jitter_ms_max": 0,
    "max_payload": 4096
  },
  "rules": [
    {
      "suffix": ".youtube.com",
      "action": "fragment",
      "tls": {
        "fragment": {
          "strategy": "sni_cut",
          "jitter_ms_min": 1,
          "jitter_ms_max": 6
        }
      }
    },
    {
      "suffix": "youtu.be",
      "action": "fragment",
      "tls": {
        "fragment": {
          "strategy": "sni_cut",
          "jitter_ms_min": 1,
          "jitter_ms_max": 6
        }
      }
    },
    {
      "suffix": ".googlevideo.com",
      "action": "fragment",
      "tls": {
        "fragment": {
          "strategy": "sni_cut",
          "jitter_ms_min": 1,
          "jitter_ms_max": 6
        }
      }
    },
    {
      "suffix": ".ytimg.com",
      "action": "fragment",
      "tls": {
        "fragment": {
          "strategy": "sni_cut",
          "jitter_ms_min": 1,
          "jitter_ms_max": 6
        }
      }
    },
    {
      "suffix": ".youtubei.googleapis.com",
      "action": "fragment",
      "tls": {
        "fragment": {
          "strategy": "sni_cut",
          "jitter_ms_min": 1,
          "jitter_ms_max": 6
        }
      }
    },
    {
      "suffix": ".googleusercontent.com",
      "action": "fragment",
      "tls": {
        "fragment": {
          "strategy": "sni_cut",
          "jitter_ms_min": 1,
          "jitter_ms_max": 6
        }
      }
    }
  ]
}